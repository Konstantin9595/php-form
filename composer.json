{
    "name": "testing",
    "description": "Respondent testing",
    "authors": [{
        "name": "Konstantin Saigushev",
        "email": "konstantin.saigushev@gmail.com"
    }],
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "require": {
        "monolog/monolog": "1.0.*",
        "tmarois/filebase": "^1.0",
        "tightenco/collect": "^5.8",
        "symfony/security": "^4.3",
        "zendframework/zend-diactoros": "^2.1",
        "form-manager/form-manager": "^6.0",
        "php-di/php-di": "^6.0",
        "relay/relay": "2.0",
        "middlewares/fast-route": "^1.2",
        "middlewares/request-handler": "^1.4",
        "faapz/pdo": "^1.12",
        "rakit/validation": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public/",
        "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php tests/"
    }
}
